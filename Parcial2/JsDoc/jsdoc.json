{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["./Controllers/mySQL.js"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "node_modules/docolatte",
        "encoding": "utf8",
        "destination": "./out/",
        "recurse": true
    }
}
